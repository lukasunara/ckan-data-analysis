<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/head'); %>
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
</head>

<body class="lightblue-bg">
    <%- include('../partials/header'); %>
    
    <%- include('../partials/spinner'); %>
    <main id="main">
        <div id="objectData" class="d-none" data-test="<%= JSON.stringify(objectData) %>"></div>
        <div class="mt-3"><h1 class="text-center">
            <%= objectData.title ? objectData.title : objectData.object_id %>
        </h1></div>
        <%- include('../partials/charts/charts'); %>

        <%- include('../partials/accordion_list', { list: objectData.organizations, typeOfObject: 'organization' }); %>
    </main>
    
    <%- include('../partials/footer'); %>

    <%- include('../partials/external_scripts'); %>
    <script src='/scripts/loading.js'></script>
</body>